<?xml version="1.0" encoding="utf-8"?>
<CodeTemplates version="3.0">
  <CodeTemplate version="2.0">
    <Header>
      <_Group>xaml</_Group>
      <Version />
      <MimeType>application/xml</MimeType>
      <Shortcut>set</Shortcut>
      <_Description>Declares a single setter attribute.</_Description>
      <TemplateType>Expansion</TemplateType>
    </Header>
    <Variables>
      <Variable name="prop">
        <Default>BackgroundColor</Default>
      </Variable>
      <Variable name="value">
        <Default>White</Default>
      </Variable>
    </Variables>
    <Code><![CDATA[<Setter Property="$prop$" Value="$value$" />]]></Code>
  </CodeTemplate>
</CodeTemplates>